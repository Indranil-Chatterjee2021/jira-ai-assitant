{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend server for JIRA AI RAG",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "env-cmd -f .env jest --detectOpenHandles",
    "test:no-env": "jest --detectOpenHandles",
    "test:e2e": "env-cmd -f .env jest src/tests/e2e.test.ts --detectOpenHandles",
    "test:debug": "env-cmd -f .env jest --detectOpenHandles --verbose --runInBand",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.7.1",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "json-schema-to-typescript": "^15.0.4",
    "langchain": "^0.0.75",
    "openai": "^4.24.0",
    "winston": "^3.8.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.11.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
