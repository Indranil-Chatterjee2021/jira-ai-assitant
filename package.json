{
  "name": "jira-ai-rag",
  "version": "1.0.0",
  "description": "JIRA AI integration with Retrieval-Augmented Generation",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "dev": "npm run sync-version && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "sync-version": "node -e \"const pkg = require('./package.json'); require('fs').writeFileSync('./client/.env', 'REACT_APP_VERSION=' + pkg.version + '\\n');\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "install:all": "npm install && npm run install:client && npm run install:server",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test"
  },
  "keywords": [
    "jira",
    "ai",
    "rag",
    "react",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.0"
  },
  "devDependencies": {}
}
